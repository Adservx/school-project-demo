{
  "version": 2,
  "buildCommand": "node node_modules/react-scripts/bin/react-scripts.js build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/glry/(.*)",
      "dest": "/glry/$1"
    },
    {
      "src": "/favicon.(ico|svg)",
      "dest": "/favicon.$1"
    },
    {
      "src": "/favicon-(16x16|32x32).png",
      "dest": "/favicon-$1.png"
    },
    {
      "src": "/apple-touch-icon.png",
      "dest": "/apple-touch-icon.png"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/logo(.*).png",
      "dest": "/logo$1.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
