{
  "version": 2,
  "buildCommand": "node node_modules/react-scripts/bin/react-scripts.js build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": null,
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/glry/(.*)",
      "destination": "/glry/$1"
    },
    {
      "source": "/favicon.(ico|svg)",
      "destination": "/favicon.$1"
    },
    {
      "source": "/favicon-(16x16|32x32).png",
      "destination": "/favicon-$1.png"
    },
    {
      "source": "/apple-touch-icon.png",
      "destination": "/apple-touch-icon.png"
    },
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/logo(.*).png",
      "destination": "/logo$1.png"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
